‚àÄ inner cells @ 2.2 { // Runup
  const ‚Ñù Œµ = option_epsd;
  const ‚Ñù coef_grad_h = coef_gradx_h;
  if (d_hn < Œµ){
    if (d_hn[‚Üí]<Œµ or hn[‚Üí]<-d) unp = 0.0;
    if (unp<0.0 and hn[‚Üí]>hn) unp -=  coef_grad_h*(hn+d[‚Üí]);
    continue;
  }
  if (d_hn[‚Üí] < Œµ){
    if (hn < -d[‚Üí]) unp = 0.0;
    if (unp>0.0 and hn>hn[‚Üí]) unp += coef_grad_h*(hn[‚Üí]+d);
    continue;
  }
  if (unp>0.0){
    if (-d>hn[‚Üí]) unp += coef_grad_h*(hn[‚Üí]+d);
    if (-d[‚Üí]>hn) unp -= coef_grad_h*(hn+d[‚Üí]);
  }
}

‚àÄ inner cells @ 3 { un = unp; }

