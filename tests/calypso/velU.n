// ***************************************************************************
// * Compute loop
// ***************************************************************************

‚àÄ outer cells @ 2.0 { unp=0.0; }

‚àÄ inner cells @ 2.0 {
  const ‚Ñù tv = ¬º*(vn[‚Üì]+vn[‚Üò]+vn+vn[‚Üí]);
  const ‚Ñù ul = (un>0.0)?un:un[‚Üí];
  const ‚Ñù ur = (un>0.0)?un[‚Üê]:un;
  const ‚Ñù uu = (tv>0.0)?un:un[‚Üë];
  const ‚Ñù ud = (tv>0.0)?un[‚Üì]:un;
  const ‚Ñù tu1 = un*(ul-ur);
  const ‚Ñù tu2 = tv*(uu-ud);
  const ‚Ñù thu = GZ*(hn[‚Üí]-hn);
  const ‚Ñù tfc = -tv*fc;
  const ‚Ñù dequ = (tu1+thu)*inv_dx + tu2*inv_dy + tfc;
  unp = un - Œ¥t*dequ;
}

‚àÄ inner east cells @ 2.1 { unp=0.0; }

/*‚àÄ cells @ 2.1 {
